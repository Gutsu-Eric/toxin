include dependencies.pug
mixin header(user)
  header.header&attributes(attributes)
    +logo
    +header__menu([
      {name: 'Home', active: true},
      {name: 'About Us'},
      {name: 'Services', more: true},
      {name: 'Career'},
      {name: 'News'},
      {name: 'Documentation'}
    ])
    +header__user-info(user)